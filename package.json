{
  "name": "hsdk",
  "version": "2.0.3",
  "description": "A hypermedia standard development kit for knowing about home api endpoints",
  "main": "transpiled/source/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf './transpiled/' && mkdir './transpiled/' && babel './source/**/index.js' -d './transpiled/'",
    "example": "browserify --verbose --transform babelify --entry './example/client.js' --outfile './example/index.js'",
    "test": "mocha --compilers js:babel-register './source/**/test.js'",
    "start": "npm-run-all --parallel watch:build watch:test",
    "lint": "eslint './source/'",
    "watch": "npm-run-all --parallel watch:test watch:lint watch:build",
    "watch:build": "chokidar './source/' --initial --command 'npm run build --silent'",
    "watch:test": "chokidar './source/' --initial --command 'npm run test --silent'",
    "watch:lint": "chokidar './source/' --initial --command 'npm run lint --silent'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krainboltgreene/hsdk.js.git"
  },
  "keywords": [
    "http",
    "api",
    "client",
    "hsdk",
    "hateoas"
  ],
  "author": "Kurtis Rainbolt-Greene <me@kurtisrainboltgreene.name>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krainboltgreene/hsdk.js/issues"
  },
  "homepage": "https://github.com/krainboltgreene/hsdk.js#readme",
  "devDependencies": {
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^7",
    "babel-plugin-relative-import": "^1",
    "babel-plugin-transform-object-rest-spread": "^6",
    "babel-preset-latest": "^6",
    "babelify": "^7",
    "browserify": "^13",
    "chai": "^3",
    "chokidar-cli": "^1",
    "eslint": "^3",
    "eslint-plugin-babel": "^4",
    "eslint-plugin-immutable": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-mocha": "^4",
    "eslint-plugin-node": "^3",
    "greenkeeper": "^4",
    "http-server": "0.9.0",
    "mocha": "^2",
    "npm": "^3",
    "npm-run-all": "^3"
  },
  "dependencies": {
    "abstraction": "^9",
    "axios": "0.15.2",
    "httpstatuses": "^3",
    "ramda": "0.21.0",
    "ramda-extra": "^1",
    "recordType": "^1",
    "url-template": "^2"
  }
}
